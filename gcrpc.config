# io_uring params
sq_entries = 64
cq_entries = 128  # cq_entries is set at least 2 * sq_entries in io_uring by default
origin_channel_size = 64

# network addr
client_ip = 127.0.0.1
port = 1214
max_conn = 8196

# about message queue
feedback_bulk_size = 1024
worker_thread = 4;

# about cache length
max_buffer_size = 256;

# about protocal version
protocal_name = gcrpc  # only gcrpc or http is supported right now
protocal_version = 1.0.0

# etcd_client
etcd_addr = http://127.0.0.1:2379